{
  "name": "bluegeek/laravel-versions",
  "description": "Provides backwards compatibility for helpers in the latest Laravel release.",
  "keywords": ["laravel", "helpers"],
  "license": "MIT",
  "authors": [
    {
      "name": "ccb",
      "email": "ccb412727@qq.com"
    }
  ],
  "require": {
    "php": ">=7.1.3"
  },
  "autoload": {
    "psr-4": {
      "Ccb\\Version\\": "/src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ccb\\Version\\CcbVersionServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
